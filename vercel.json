{
  "installCommand": "npm install --legacy-peer-deps",
  "version": "1.0.0",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest", "/"
    }
  ]
}
